{
    "content_scripts": [
        {
            "matches": [
                "*://*.youtube.com/*"
            ], 
            "run_at": "document_start", 
            "js": [
            	"lib/jquery-3.2.1.js",
            	"js/getLikes.js"
            ]
        }
    ], 
    "name": "youtubeLikes", 
    "version": "1.0", 
    "manifest_version": 2, 
    "permissions": [
        "activeTab"
    ], 
    "browser_action": {
        "default_icon": "img/thumbsup.png"
    },
    "description": ""
}